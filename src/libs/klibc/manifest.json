{
    "$schema": "https://schemas.cute.engineering/stable/osdk.manifest.component.v1",
    "id": "klibc",
    "description": "Libc compatibility layer for the kernel (useful for data structures)",
    "type": "lib",
    "props": {
        "cpp-root-include": true
    },
    "enableIf": {
        "freestanding": [
            true
        ]
    },
    "provides": [
        "libc"
    ]
}